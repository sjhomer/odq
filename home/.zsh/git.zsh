# Git helpers
alias gas='gh auth status'
alias gp='git push'
alias gf='git fetch --all'
alias gpl='gf && git pull'
alias go='git checkout'
alias gbdel='git branch | xargs git branch -D'
alias ga='git add .'
alias gs='git status'
